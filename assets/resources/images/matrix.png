      top:$("#top"+id_class).val(),
      id_scorecard:$("#id_scorecard"+id_class).val()
    },
    success:function(ret){
      reqtb(id_class, id_kajian_risiko);
      $("#kesimpulan"+id_class).modal('toggle');
    }
  });
}
